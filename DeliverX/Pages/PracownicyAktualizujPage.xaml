        <TextBox x:Name="DataUrodzeniaTextBox" HorizontalAlignment="Left" Height="22" Margin="184,166,0,0" Padding="0 2 0 0" TextWrapping="Wrap" VerticalAlignment="Top" Width="122" FontSize="10" Style="{DynamicResource BlueTextBox}" IsEnabled="False"/>
        <TextBox x:Name="DataZatrudnieniaTextBox" HorizontalAlignment="Left" Height="22" Margin="184,196,0,0" Padding="0 2 0 0" TextWrapping="Wrap" VerticalAlignment="Top" Width="122" FontSize="10" Style="{DynamicResource BlueTextBox}" IsEnabled="False"/>
