        <Button x:Name="ResetButton" Content="RESET" Click="ResetButton_Click" FontWeight="Bold" Margin="0,448,115,10" Style="{DynamicResource BlueButton}" HorizontalAlignment="Right" Width="86"/>
    </Grid>
    </ScrollViewer>
</Page>

